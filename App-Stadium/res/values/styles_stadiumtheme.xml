<?xml version="1.0" encoding="utf-8"?>

<!-- Generated with http://android-holo-colors.com -->
<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- Action bar -->
    <style name="ActionBar.Solid.Stadiumtheme" parent="@android:style/Widget.Holo.Light.ActionBar.Solid.Inverse">
        <item name="android:background">@drawable/ab_solid_stadiumtheme</item>
        <item name="android:backgroundStacked">@drawable/ab_stacked_solid_stadiumtheme</item>
        <item name="android:backgroundSplit">@drawable/ab_bottom_solid_stadiumtheme</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Stadiumtheme</item>
    </style>

    <style name="ActionBar.Transparent.Stadiumtheme" parent="@android:style/Widget.Holo.ActionBar">
        <item name="android:background">@drawable/ab_transparent_stadiumtheme</item>
        <item name="android:progressBarStyle">@style/ProgressBar.Stadiumtheme</item>
    </style>

    <style name="PopupMenu.Stadiumtheme" parent="@android:style/Widget.Holo.ListPopupWindow">
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_stadiumtheme</item>
    </style>

    <style name="DropDownListView.Stadiumtheme" parent="@android:style/Widget.Holo.ListView.DropDown">
        <item name="android:listSelector">@drawable/selectable_background_stadiumtheme</item>
    </style>

    <style name="ActionBarTabStyle.Stadiumtheme" parent="@android:style/Widget.Holo.ActionBar.TabView">
        <item name="android:background">@drawable/tab_indicator_ab_stadiumtheme</item>
    </style>

    <style name="DropDownNav.Stadiumtheme" parent="@android:style/Widget.Holo.Spinner">
        <item name="android:background">@drawable/spinner_background_ab_stadiumtheme</item>
        <item name="android:popupBackground">@drawable/menu_dropdown_panel_stadiumtheme</item>
        <item name="android:dropDownSelector">@drawable/selectable_background_stadiumtheme</item>
    </style>

    <style name="ProgressBar.Stadiumtheme" parent="@android:style/Widget.Holo.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/progress_horizontal_stadiumtheme</item>
    </style>

    <style name="ActionButton.CloseMode.Stadiumtheme" parent="@android:style/Widget.Holo.ActionButton.CloseMode">
        <item name="android:background">@drawable/btn_cab_done_stadiumtheme</item>
    </style>

    <!-- this style is only referenced in a Light.DarkActionBar based theme -->
    <style name="Theme.Stadiumtheme.Widget" parent="@android:style/Theme.Holo">
        <item name="android:popupMenuStyle">@style/PopupMenu.Stadiumtheme</item>
        <item name="android:dropDownListViewStyle">@style/DropDownListView.Stadiumtheme</item>
    </style>

    <!-- Ui Widgets -->
    <style name="AutoCompleteTextViewStadiumTheme" parent="android:Widget.Holo.Light.AutoCompleteTextView">
        <item name="android:dropDownSelector">@drawable/stadiumtheme_list_selector_holo_light</item>
        <item name="android:background">@drawable/stadiumtheme_edit_text_holo_light</item>
    </style>

    <style name="ButtonStadiumTheme" parent="android:Widget.Holo.Light.Button">
        <item name="android:background">@drawable/btn_default_selector</item>
        <item name="android:textColor">@android:color/white</item>
    </style>

    <style name="ImageButtonStadiumTheme" parent="android:Widget.Holo.Light.ImageButton">
        <item name="android:background">@drawable/stadiumtheme_btn_default_holo_light</item>
    </style>

    <style name="SpinnerStadiumTheme" parent="android:Widget.Holo.Light.Spinner">
        <item name="android:background">@drawable/stadiumtheme_spinner_background_holo_light</item>
        <item name="android:dropDownSelector">@drawable/stadiumtheme_list_selector_holo_light</item>
    </style>

    <style name="ProgressBarStadiumTheme" parent="android:Widget.Holo.Light.ProgressBar.Horizontal">
        <item name="android:progressDrawable">@drawable/stadiumtheme_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/stadiumtheme_progress_indeterminate_horizontal_holo_light</item>
    </style>

    <style name="SeekBarStadiumTheme" parent="android:Widget.Holo.Light.SeekBar">
        <item name="android:progressDrawable">@drawable/stadiumtheme_scrubber_progress_horizontal_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/stadiumtheme_scrubber_progress_horizontal_holo_light</item>
        <item name="android:thumb">@drawable/stadiumtheme_scrubber_control_selector_holo_light</item>
    </style>

    <style name="RatingBarStadiumTheme" parent="android:Widget.Holo.Light.RatingBar">
        <item name="android:progressDrawable">@drawable/stadiumtheme_ratingbar_full_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/stadiumtheme_ratingbar_full_holo_light</item>
    </style>

    <style name="RatingBarBigStadiumTheme" parent="android:Widget.Holo.Light.RatingBar.Indicator">
        <item name="android:progressDrawable">@drawable/stadiumtheme_ratingbar_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/stadiumtheme_ratingbar_holo_light</item>
    </style>

    <style name="RatingBarSmallStadiumTheme" parent="android:Widget.Holo.Light.RatingBar.Small">
        <item name="android:progressDrawable">@drawable/stadiumtheme_ratingbar_small_holo_light</item>
        <item name="android:indeterminateDrawable">@drawable/stadiumtheme_ratingbar_small_holo_light</item>
    </style>

    <style name="ToggleStadiumTheme" parent="android:Widget.Holo.Light.Button.Toggle">
        <item name="android:background">@drawable/stadiumtheme_btn_toggle_holo_light</item>
    </style>

</resources>